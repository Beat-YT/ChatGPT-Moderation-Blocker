{
  "description": "This extension block text content from getting flagged or blocked by the filter.",
  "name": "Chat GPT anti censorship",
  "version": "3.1",
  "permissions": [
    "declarativeNetRequest",
    "scripting"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "host_permissions": [
    "*://chat.openai.com/*",
    "*://api.openai.com/*"
  ],
  "content_scripts": [
    {
      "world": "MAIN",
      "js": ["modapi-disabler.js"],
      "matches": ["https://chat.openai.com/*"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "128": "icons/128.png"
  },
  "manifest_version": 3,
  "key": ""
}