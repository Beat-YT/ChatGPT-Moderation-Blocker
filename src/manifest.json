{
  "description": "This extension block text content from getting flagged or blocked by the filter.",
  "name": "Chat GPT anti censorship",
  "version": "2.2",
  "permissions": [
    "declarativeNetRequest"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "host_permissions": [
    "*://chat.openai.com/*",
    "*://api.openai.com/*"
  ],
  "manifest_version": 3
}